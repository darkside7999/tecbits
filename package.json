{
  "name": "tecbits",
  "version": "1.0.0",
  "description": "Blog de noticias moderno y minimalista construido con Next.js, TypeScript y Tailwind CSS",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
